class parent:
    def __int__(self, material, length, width, height, price):
      self.__mateiral = material
      self.__length = length
      self.__width = width
      self.__height = height
      self.__price = price

class desk(parent):

    def set_material(self, material):
      self.__mateiral = material
    def set_length(self, length):
      self.__length = length
    def set_width(self, width):
      self.__width = width
    def set_height(self, height):
      self.__height = height
    def set_price(self, price):
      self.__price = price

    def get_material(self):
      return self.__material
    def get_length(self):
      return self.__length
    def get_width(self):
      return self.__width
    def get_height(self):
      return self.__height
    def get_price(self)
      return self.__price


    def __str__(self):
      line_item = self .__mateiral + " qty-" + str(self.__length) + " each ${-0,.2f}".format{self._price} + " total= $(:0,.2f)".format(self.get)
      return line_item

    leftdesk = desk("Wooden, 2 drawers, 15ft High. 5ft Wide", 34, .99)
