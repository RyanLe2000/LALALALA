class person1:

    def __init__(self, age, adress, number):
        self.age = age
        self.adress = adress
        self.number = number
        self.info = False
    age = int(18)
    adress = str("10902 Golden Gate")
    number = str('309-989-2202')
    name = str("Ryan")
    print(name)
    print(age)
    print(adress)
    print(number)
    print("-----------------------------")
    
